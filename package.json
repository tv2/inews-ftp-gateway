    "@types/dotenv": "^6.1.1",
    "@types/request-promise": "^4.1.44",
    "@types/xml2js": "^0.4.4",
    "acorn": "^7.1.1",
    "clone": "^2.1.2",
    "dotenv": "^8.0.0",
    "handlebars": "^4.3.0",
    "inews": "https://github.com/SuperFlyTV/node-inews.git#20200917_2",
    "jobs-queue": "git+https://github.com/sparkpunkd/node-jobs-queue.git#master",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "timeline-state-resolver-types": "git+https://github.com/olzzon/tv-automation-state-timeline-resolver#dist_types20200922_1",
    "tslib": "^2.0.0",
    "tv-automation-server-core-integration": "git+https://github.com/olzzon/tv-automation-server-core-integration.git#v20200910_1",
    "tv-automation-sofie-blueprints-integration": "git+https://github.com/olzzon/tv-automation-sofie-blueprints-integration.git#dist20200911_3",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "winston": "^2.4.2",
    "xml2js": "^0.4.19"
  },
  "resolutions": {
    "typedoc/**/marked": "^0.7.0",
    "handlebars": "^4.3.0",
    "jobs-queue": "git+https://github.com/sparkpunkd/node-jobs-queue.git#master",
    "kind-of": "^6.0.3",
    "acorn": "^7.1.1",
    "minimist": "^0.2.1"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.12.64",
    "@types/underscore": "^1.8.13",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.6.1",
    "gh-pages": "^2.0.1",
    "jest": "^24.5.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "mkdirp": "^0.5.1",
    "node-license-validator": "^1.3.0",
    "npm-scripts-info": "^0.3.9",
    "open-cli": "^5.0.0",
    "prettier": "2.1.2",
    "standard-version": "^6.0.1",
    "trash-cli": "^3.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript-tslint-plugin": "^0.5.5",
    "typedoc": "^0.14.2",
    "typescript": "^3.7.2"
  },
  "standard-version": {
    "message": "chore(release): %s [skip ci]",
    "tagPrefix": ""
  }
}
